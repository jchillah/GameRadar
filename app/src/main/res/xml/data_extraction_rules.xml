<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Backup-Regeln fÃ¼r GameFinder App -->
        <include
            domain="database"
            path="game_database" />
        <include
            domain="sharedpref"
            path="settings" />
        <include
            domain="file"
            path="cache" />
        <exclude
            domain="file"
            path="temp" />
        <exclude
            domain="file"
            path="logs" />
    </cloud-backup>
    <device-transfer>
        <include
            domain="database"
            path="game_database" />
        <include
            domain="sharedpref"
            path="settings" />
        <include
            domain="file"
            path="cache" />
        <exclude
            domain="file"
            path="temp" />
        <exclude
            domain="file"
            path="logs" />
    </device-transfer>
</data-extraction-rules>